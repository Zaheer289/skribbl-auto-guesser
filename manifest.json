{
    "manifest_version":3,
    "version":"1.0.0",
    "name":"Skribbl.io Auto Word Guesser!",
    "description":"Automatically guesses words on your behalf, enabling you to win games effortlessly!",
    "icons":{
        "16":"images/icon-16.png",
        "32":"images/icon-16.png",
        "48":"images/icon-16.png",
        "128":"images/icon-16.png"
    },
    "content_scripts" : [
        {
            "js": ["js/content.js"],
            "matches":["https://skribbl.io/*"]
        }
    ]
}